<div class="row main-row" ng-class-odd="'brws-item--dark'">
	  <!-- HEADING -->
	<div class="col-sm-12 text-center">
	  <h2 class="stripe__title">{{stripe.title}} <a class="view-all-link" ng-if="stripe.total > stripe.per_page && stripe.extra_fields.view_all">Vedi tutti</a></h2>
	</div>
    <div class="col-xs-12" ng-repeat="contents in [stripe.contents]">
	    <%= render partial: "/application/content_carousel" %>
	</div>
</div>
