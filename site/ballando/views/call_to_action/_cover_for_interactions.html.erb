<div class="col-sm-12">
  <div class="row text-center cta-stream-heading" style="position: relative;">
    <div id="calltoaction-<%= calltoaction.id %>-countdown" class="col-sm-12" style="padding-top: 5px;">
      <h3></h3>
    </div>
    <div class="col-sm-12 calltoaction-cover-description">
      <h4 style="font-size: 16px;">
        <h3><%= calltoaction.description %></h3>
      </h4>
    </div>
    <div class="col-xs-12 text-center undervideo-feedback">
      <h5>Ottieni fino a +<%= calltoaction_reward_status[:winnable_reward_count] %> punti</h5>
      <% unless calltoaction_reward_status[:reward].numeric_display %>  
      <div class="col-sm-12 text-center" style="margin-bottom: 30px;">  
        <% calltoaction_reward_status[:reward_status_images].each do |reward| %>
            <img src="<%= reward %>" style="width: 30px;">
        <% end %>
      </div>
    <% end %>
    </div>
  </div>
</div>