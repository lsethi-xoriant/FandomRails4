<style type="text/css">

  <% if aux['property_info']['extra_fields']['image_background'] %>
    body {
      background-image: url("<%= aux['property_info']['extra_fields']['image_background']['url'] %>");
      background-position: top center;
      background-repeat: no-repeat;
      background-attachment: fixed;
      background-size: 100% auto;
    }
  <% end %>

  .section-heading__marker {
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .append-comments {
    color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .btn-filter {
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
    border-color: <%= darken_color(aux["property_info"]['extra_fields']["background_color"]) %> !important;
  }

  .cta__sidebar-panel--sign-up__title {
    color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .btn-filter:hover {
    background-color: <%= lighten_color(aux["property_info"]['extra_fields']["background_color"], 0.5) %> !important;
  }

  .profile-header__item__rank-position {
    color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .ranking-element__position {
    color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .relateds__title a, .section-heading__see-all {
    color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .progress-bar {
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .property-border {
    border: 1px solid <%= aux["property_info"]['extra_fields']["background_color"] %>;
  }

  .percentage_circle_bar, .percentage_circle_fill {
    border-color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .percentage_circle {
    background-color: <%= lighten_color(aux["property_info"]['extra_fields']["background_color"], 0.5) %> !important;
  }

  .percentage_circle:after {
    background-color: #333 !important;
  }

  .reward-catalogue-header {
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .profile-menu-block {
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .profile-menu-block__item--active {
    background-color: <%= darken_color(aux["property_info"]['extra_fields']["background_color"]) %> !important;
  }

  .profile-menu-block__item:hover {
    background-color: <%= darken_color(aux["property_info"]['extra_fields']["background_color"]) %> !important;
  }

  .relateds-flexslider__item-panel:hover .cta-preview__title {
    color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .chapter, .pin-interaction__pin, .relateds__icon-container {
    background: <%= aux["property_info"]['extra_fields']["background_color"] %>; 
  }

  .cta-cover__media_type {
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .dropdown-menu > .active > a, .dropdown-menu > .active > a:hover, .dropdown-menu > .active > a:focus {
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .navbar-item--socials .navbar-item--socials-container .fa:hover {
    color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .active .ctas__search__title {
    color: #fff !important;
  }

  .ranking-element--myposition {
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .navbar-default .navbar-nav > .active > a,
  .navbar-default .navbar-nav > li > a:hover,
  .navbar-default .navbar-nav > li > a:focus {
    color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
    background-color: #000;
  }

  .properties__flexslider .flex-direction-nav a.flex-prev:before,
  .properties__flexslider .flex-direction-nav a.flex-next:before {
    color: <%= aux["property_info"]['extra_fields']["background_color"] %> !important;
  }

  .btn-primary { 
    color: #FFFFFF; 
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %>; 
    border-color: <%= aux["property_info"]['extra_fields']["background_color"] %>; 
  } 
   
  .btn-primary:hover, 
  .btn-primary:focus, 
  .btn-primary:active, 
  .btn-primary.active, 
  .open .dropdown-toggle.btn-primary { 
    color: #FFFFFF; 
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %>; 
    border-color: <%= aux["property_info"]['extra_fields']["background_color"] %>; 
  } 
   
  .btn-primary:active, 
  .btn-primary.active, 
  .open .dropdown-toggle.btn-primary { 
    background-image: none; 
  } 
   
  .btn-primary.disabled, 
  .btn-primary[disabled], 
  fieldset[disabled] .btn-primary, 
  .btn-primary.disabled:hover, 
  .btn-primary[disabled]:hover, 
  fieldset[disabled] .btn-primary:hover, 
  .btn-primary.disabled:focus, 
  .btn-primary[disabled]:focus, 
  fieldset[disabled] .btn-primary:focus, 
  .btn-primary.disabled:active, 
  .btn-primary[disabled]:active, 
  fieldset[disabled] .btn-primary:active, 
  .btn-primary.disabled.active, 
  .btn-primary[disabled].active, 
  fieldset[disabled] .btn-primary.active { 
    background-color: <%= aux["property_info"]['extra_fields']["background_color"] %>; 
    border-color: <%= aux["property_info"]['extra_fields']["background_color"] %>; 
  } 

</style>